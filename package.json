{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "test": "npm run before_test && NODE_ENV=test nyc --reporter=text mocha --require babel-register --require babel-polyfill ./tests/* --timeout 8000 --exit",
    "before_test": "NODE_ENV=test npm run drop_tables && NODE_ENV=test npm run create_tables && NODE_ENV=test sequelize db:seed:all",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "drop_tables": "sequelize db:migrate:undo:all",
    "create_tables": "sequelize db:migrate",
    "start": "node dist/index.js",
    "build": "sequelize db:migrate && babel src --out-dir dist",
    "dev": "nodemon --exec babel-node ./src/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@rucken/core-nestjs": "^1.0.12",
    "@sendgrid/mail": "^6.4.0",
    "async": "^3.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "googleapis": "^40.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.2.1",
    "nyc": "^14.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-mock": "0.0.3",
    "passport-mocked": "^1.3.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.2",
    "request": "^2.87.0",
    "sendgrid": "^5.2.3",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "swagger-jsdoc": "^1.3.0",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.7.6",
    "nodemon": "^1.19.1"
  }
}
